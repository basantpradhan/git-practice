   			 $(selector).append($(selector).css('background','#659FDA')).append($(selector).stop(true).fadeTo(1,200));
	},

function(){

   $(selector).append(blinkeffect(selector,true));

}); 



$(selector).fadeOut('fast', function() {

$(this).fadeIn('slow', function() {

blinkeffect(this,true); 
//blinkEffectOnPinClick(this);
});
});
}
</script>

