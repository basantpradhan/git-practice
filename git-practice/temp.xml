		    	    		}
		    	    
			   }, 
			   error:function(e){
				   
				   $('#uuid').html('some error occured');
				   }
			});
  }
</script>

<script type="text/javascript">
 
 
function searchByAddress(){
       
       $('.error').css('display', 'none');
       $('.searchResultDiv').html('please wait loading....');
       $('.searchResultDiv').css('display','block');
       
       
       var community = $('#search_city').val().trim();
       var street = $('#search_street').val().trim();
       var poi = $('#search_poi').val().trim();
       
       var locale = $('#cur_locale').attr('value').split('_');
       var lang = locale[0];
     
     type = "poi";
     address = poi;
     pos=$('#search_poi').position();
     
     dynamicSerachDivWidth = ($('#search_poi').width());
     $('.searchResultDiv').css('width',(dynamicSerachDivWidth+2)+'px');
     
     txt_field = 3;
              
                     if ($("#search_street").is(':focus'))
                     {
                           type="stnw";
                           address = street;
                           pos = $('#search_street').position();
                           txt_field = 2;
                           
                     }
                     else if ($("#search_poi").is(':focus'))
                     {
                             type="poi";
                              pos=$('#search_poi').position();
                             txt_field = 3;
                             
                     }
                     else if ($("#search_city").is(':focus')){
                            type='comm';
                            address=community;
                             pos = $('#search_city').position();
                             txt_field = 1;
                           
                     }else{
                           
                     if (community!=''){
                     
                             type="comm";
                             address=community;
                             pos = $('#search_city').position();
                             txt_field = 1;
                          }        
                        else if(street!=''){
                              
                             type="stnw";
                             address = street;
                             pos = $('#search_street').position();
                             txt_field = 2;
                             
                          }
                        else if(poi!=''){
                             type="poi";
                             address=poi;
                             pos = $('#search_poi').position();
                             txt_field = 3;
                          }
                        else {
                              type="comm";
                              address=community;
                              pos = $('#search_city').position();
                              txt_field = 1;
                             
                           }
                     }
     
                     err_msg = "";
                     
                     if(txt_field==1)
                    	 err_msg = $('#hid_err_comm_nt_found').val();
                     if(txt_field==2)
                    	 err_msg = $('#hid_err_strt_nt_found').val();
                     if(txt_field==3)
                    	 err_msg = $('#hid_err_poi_nt_found').val();
                     $('#error_searchByAddr').html(err_msg); 
                     
                    
                     if(address=='?'){
                    	 
                    	 
                         $('#error_searchByAddr').css('display','block');
                         if(lang=='ar')
                                $('#error_searchByAddr').css('float','right');
                        else
                         $('#error_searchByAddr').css('float','left');
                         
                         $('.searchResultDiv').css('display','none');
                         return false ;
                     }
                    
                     
                     $('#hid_src_txt_focus').val(txt_field);
                     var URL = 'http://www.tracknfind.me/api/json/geonames_json.php?lang='+lang+'&type='+type+'&q='+address;
  
                     $('#error_searchByAddr').css('display','none'); 
       
       $.ajax({
              type: 'GET',
              url: URL,
              crossDomain: true,
              dataType: 'jsonp',
              success: function(response) { 
                     
                     var count = 0;
                        var content = "<table>";
                        for(var i=0;i<response.length;i++){
       
                             count = count +1 ;
                             var results = JSON.stringify(response[i]);
                             
                              op = JSON.parse(results);
                             
                            
                              lat = op.y_coord;
                              lon = op.x_coord;
                            
                              if(type=='comm'){
                              
                                      if(lang=='ar')
                                      frmt_addr = op.comm_name2;
                                   else
                                         frmt_addr=op.comm_name1;
                              
                              }else if(type=="stnw"){
                                     
                                     if(lang=='ar')
                                      frmt_addr = op.name2;
                                   else
                                         frmt_addr=op.name1;
                              }else{
                                     
                                     if(lang=='ar')
                                      frmt_addr = op.name2;
                                   else
                                         frmt_addr=op.name1;
                                     
                              }
                              
                            
                               content = content +"<tr >";
                               if(lang=='ar')
                                    content = content +"<td><span style=float:right;margin-right:10px;margin-top:1px;font-size:12px; lat="+lat+" lon="+lon+" addr='"+frmt_addr+"'  class=srch_result>"+frmt_addr+"</span></td>";
                                  else
                                           content = content +"<td><span style=float:left;margin-right:10px;margin-top:1px;font-size:12px; lat="+lat+" lon="+lon+" addr='"+frmt_addr+"' class=srch_result>"+frmt_addr+"</span></td>";
                                  
                                  content = content +"</tr>"; 
                                  
                   }
                     
                         
                        search_txt = "";
                        
                        if(txt_field==3)
                              search_txt = $('#search_poi').val();
                        else if(txt_field==2)
                              search_txt = $('#search_street').val();
                        else if(txt_field==1)
                              search_txt = $('#search_city').val();
                        
                        $('.txtbox_search').val('');
                        if(txt_field==3)
                             $('#search_poi').val(search_txt);
                        else if(txt_field==2)
                               $('#search_street').val(search_txt);
                        else if(txt_field==1)
                              $('#search_city').val(search_txt);
                        
                        if(count==0){
                           
                              $('#error_searchByAddr').css('display','block');
                              if(lang=='ar')
                                     $('#error_searchByAddr').css('float','right');
                             else
                              $('#error_searchByAddr').css('float','left');
                           
                           
                              
                        }
                              
                       content = content +"</table>";
                       $('.searchResultDiv').html(content);
                       if(response.length!=0)
                         $('.searchResultDiv').css('display','block');
                       else
                             $('.searchResultDiv').css('display','none');
                     
                       
                       var hgt = pos.top+24;
                       
                       $('.searchResultDiv').css('top',hgt+'px');
                       
                      // if(lang=='ar')
                             $('.searchResultDiv').css('left',pos.left+'px');
                     //  else
                           // $('.searchResultDiv').css('left',pos.left+'px');
                       
                     
                      
                      if(count==0){
                           $('.searchResultDiv').css('display','none');
                     }
                 }
              });
}
 
</script>


