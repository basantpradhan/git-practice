<script type="text/javascript">
  
  function saveFeedback(url){
	  
	  
	  var isSubmitted = $('#isSubmitted').val();
	  
	  if(isSubmitted!='0'){
		  return false;
	  }
	  
	  $('#isSubmitted').val('1');
	
	  $.ajax({
		 	  type:"GET",
		      url :url,
