function validateDecimal(textboxIn)
{
    var text = textboxIn.value;
    
    for(var x = 0; x < 10; x++)
    {
        while(text.indexOf(x) != -1)
        {
            text = text.replace(x,"");
        }
    }
       
    if(text != "." && text != "")
    {
        textboxIn.value = "";
    }
}
	
	
